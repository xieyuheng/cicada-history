#!/usr/bin/env bash

build () {
    cargo build --target wasm32-unknown-unknown
    wasm-bindgen ../../target/wasm32-unknown-unknown/debug/cicada_notebook.wasm --out-dir .
}

update () {
    cargo update
    npm install
}

serve () {
    npx webpack-dev-server
}

dist () {
    npx webpack
}

main () {
    for TASK in $@
    do
        $TASK
    done
}

main $@
